<section>
	<div class="container">
		<div class="row align-items-center">
			<div class="col-md-6">
				{% if ce.title %}
					<h1>
						{{ ce.title }}
					</h1>
				{% endif %}
			</div>
			<div class="col-md-6">
				{% if ce.text %}
					<p>
						{{ ce.text }}
					</p>
				{% endif %}
			</div>
		</div>
	</div>
</section>
